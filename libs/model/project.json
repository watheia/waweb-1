{
  "root": "libs/model",
  "sourceRoot": "libs/model/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/model",
        "main": "libs/model/src/index.ts",
        "tsConfig": "libs/model/tsconfig.lib.json",
        "assets": ["libs/model/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/model/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/model"],
      "options": {
        "jestConfig": "libs/model/jest.config.js",
        "passWithNoTests": true
      }
    },
    "generate": {
      "builder": "@nx-tools/nx-prisma:generate",
      "options": {
        "schema": "libs/model/schema.prisma"
      }
    },
    "migrate": {
      "builder": "@nx-tools/nx-prisma:migrate",
      "options": {
        "schema": "libs/model/schema.prisma"
      }
    },
    "seed": {
      "builder": "@nx-tools/nx-prisma:seed",
      "options": {
        "script": "libs/model/src/seed.ts",
        "tsConfig": "libs/model/tsconfig.lib.json",
        "schema": "libs/model/schema.prisma"
      }
    }
  },
  "tags": []
}
